<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjyt.dataDeal.dao.PaymentInfoDao">


    <select id="queryAll" resultType="PaymentInfo">
        select *
        from payment_info
    </select>
    <select id="queryPage" resultType="PaymentInfo">
        select *
        from payment_info

        limit #{offset},#{limit}
    </select>


</mapper>