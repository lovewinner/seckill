<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjyt.taget.dao.OldPayinfoDao">


  <select id="queryAll" resultType="OldPayinfo">
    select *
    from old_payinfo
  </select>
  <select id="queryPage" resultType="OldPayinfo">
    select *
    from old_payinfo

    limit #{offset},#{limit}
  </select>


</mapper>